{
  "name": "denote__obsidian",
  "version": "5.1.0",
  "description": "This is a sample plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && bun run esbuild.config.mjs production",
    "dev": "bun run esbuild.config.mjs",
    "version": "bun run version-bump.mjs && git add manifest.json versions.json && bun run format:fix",
    "format:fix": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "keywords": [
    "denote",
    "obsidian"
  ],
  "author": "Juan David Merchan Torres (NikolaM-Dev)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "prettier": "3.6.2",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@formkit/tempo": "0.1.2",
    "neverthrow": "8.2.0"
  }
}
